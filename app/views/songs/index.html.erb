<h1><%= @artist.name %> Songs</h1>

<%= link_to 'New Song', new_artist_song_path(@artist) %>
<ul>
  <% @songs.each do |song| %>
    <li>
      <%= link_to song.title, [@artist, song] %>
      <br />
      <%= link_to 'Edit', edit_artist_song_path(@artist, song) %>
      <%= button_to 'Delete', [@artist, song], method: :delete %>
    </li>
    <br />
  <% end %>
</ul>

<br />
<%= link_to 'Back', billboard_artist_path(@artist.billboard ,@artist) %>
