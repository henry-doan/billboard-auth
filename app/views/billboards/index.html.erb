<%= button_to "Log Out", destroy_user_session_path, method: :delete %>

<h1>Billboards</h1>
<%= link_to 'New Billboard', new_billboard_path %>
<ul>
  <% @billboards.each do |billboard| %>
    <li>
      <%= link_to billboard.name, billboard %>
      <br />
      <%= link_to 'Edit', edit_billboard_path(billboard) %>
      <%= button_to 'Delete', billboard, method: :delete %>
    </li>
    <br />
  <% end %>
</ul>
